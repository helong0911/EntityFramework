{
    "version": "7.0.0-*",
    "description":  "Shared Entity Framework components for relational data stores.",
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "EntityFramework.Core": "7.0.0-*"
    },
    "compile": "..\\Shared\\*.cs",
    "namedResource": {
        "EntityFramework.Relational.Strings": "Properties/Strings.resx"
    },
    "frameworks": {
        "net451": {
            "frameworkAssemblies": {
                "System.Data": "",
                "System.Transactions": ""
            }
        },
        "dnx451": {
            "frameworkAssemblies": {
                "System.Data": ""
            }
        },
        "dnxcore50": {
            "dependencies": {
                "Microsoft.CSharp": "4.0.0-beta-*",
                "System.Data.Common": "4.0.0-beta-*",
                "System.Text.RegularExpressions": "4.0.10-beta-*"
            }
        },
        "netcore451": {
            "dependencies": {
                "System.Data.Common": "4.0.0-beta-*"
            },
            "frameworkAssemblies": {
                "Microsoft.CSharp": "",
                "System.Dynamic.Runtime": "",
                "System.Text.RegularExpressions": ""
            }
        }
    }
}
