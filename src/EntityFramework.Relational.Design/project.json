{
    "version": "7.0.0-*",
    "description":  "Shared Design-time Entity Framework components for relational data stores.",
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "EntityFramework.Core": "7.0.0-*",
        "EntityFramework.Relational": "7.0.0-*"
    },
    "compile": "..\\Shared\\*.cs",
    "namedResource": {
        "EntityFramework.Relational.Design.Strings": "Properties/Strings.resx"
    },
    "frameworks": {
        "net451": {
            "frameworkAssemblies": {
                "System.Data": ""
            }
        },
        "dnx451": {
            "frameworkAssemblies": {
                "System.Data": ""
            }
        },
        "dnxcore50": {
            "dependencies": {
                "Microsoft.CSharp": "4.0.0-beta-*",
                "System.Data.Common": "4.0.0-beta-*",
                "System.Data.SqlClient": "4.0.0-beta-*",
                "System.Reflection.Extensions": "4.0.0-beta-*",
                "System.Text.RegularExpressions": "4.0.10-beta-*"
            }
        }
    }
}
